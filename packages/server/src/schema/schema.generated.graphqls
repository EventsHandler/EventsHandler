scalar DateTime

type Event {
  createdAt: DateTime!
  creator: User!
  description: String!
  id: ID!
  participants: [User!]!
  title: String!
}

type Mutation {
  createEvent(title: String!): Event!
  login(email: String!): User
  register(email: String!): User
}

type Query {
  events(filter: String): [Event!]!
}

type User {
  createdAt: DateTime!
  createdEvents: [Event!]!
  email: String!
  events: [Event!]!
  id: ID!
  password: String!
  username: String!
}
